# Enable debug mode to see current simulated time
VITE_DEBUG=true

# Set a specific mock time (ISO format)
# This will use this exact time for calculating the bunny's position
# Example: 7:30 AM on Easter 2025
VITE_MOCK_TIME=2025-04-20T07:30:00Z

# Other times to try:
# VITE_MOCK_TIME=2025-04-20T00:00:00Z  # Start of Easter (midnight)
# VITE_MOCK_TIME=2025-04-20T06:00:00Z  # Early morning (6 AM)
# VITE_MOCK_TIME=2025-04-20T12:00:00Z  # Noon
# VITE_MOCK_TIME=2025-04-20T18:00:00Z  # Evening (6 PM)
# VITE_MOCK_TIME=2025-04-20T23:59:59Z  # End of Easter